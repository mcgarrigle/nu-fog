
function dump {
  echo
  echo // $1
  echo
  cat $2
}

dump user-data      /tmp/tmp.gB11C4LTai
dump meta-data      /tmp/tmp.lxJWEzU9Sa
dump network-config /tmp/tmp.Saf8Rhozxn

virt-install \
  --import \
  --name tt \
  --virt-type kvm \
  --boot hd \
  --osinfo rocky9 \
  --memory 4096 \
  --vcpus 2 \
  --disk vol=filesystems/tt.qcow2,device=disk \
  --network network=bridge \
  --graphics none \
  --noautoconsole

